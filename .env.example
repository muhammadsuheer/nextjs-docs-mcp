# Upstash Redis Configuration (Optional - for caching)
# Get these from: https://console.upstash.com

# For Vercel Deployment (Auto-configured by Vercel)
KV_REST_API_URL=your_upstash_redis_url
KV_REST_API_TOKEN=your_upstash_redis_token

# For Local Development (Manual Configuration)
UPSTASH_REDIS_REST_URL=your_upstash_redis_url
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_token

# MCP Server Configuration (Optional)
MCP_VERBOSE_LOGS=false

# Note: Server works perfectly without Redis - caching is optional
# Redis provides ~2x faster response times for repeated queries
